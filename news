		<script type="text/javascript">

		jQuery(document).ready(function(){
			var jQuerycontainer = jQuery('#zentoolslist122');

							jQuerycontainer.imagesLoaded( function(){
					jQuerycontainer.masonry({
						itemSelector: '#zentoolslist122 li',
						isResizable: true,
						isAnimated: true,
						columnWidth: (jQuerycontainer.width() - 15) / 3					});
				});
				jQuery(window).resize(function(){
					var windowsize = jQuery(window).width();

					if(windowsize > 600) {
						jQuery("#zentoolslist122 li");
							jQuerycontainer.masonry({
								isResizable: true,
								isAnimated: true,
								columnWidth: jQuerycontainer.width() / 3						});
					}
					else {
						jQuery("#zentoolslist122 li");
								jQuerycontainer.masonry({
								isResizable: true,
								isAnimated: true,
								columnWidth: jQuerycontainer.width() / 1
						});
					}
				});
			
			jQuery('#jbToggle').click(function(){
				// We use this as a hook for templates to trigger and retrigger the masonry layout
				setTimeout( function() {
					var jQuerycontainer = jQuery('#zentoolslist122');

					jQuerycontainer.masonry({
						itemSelector: '#zentools122 li',
						isResizable: true,
						isAnimated: true,
						columnWidth: jQuerycontainer.width() / 3						});
					}, 500 );
				});
		});
	</script>
	

	

	
	
	

	
	

			<script type="text/javascript">
		jQuery(document).ready(function(){

			var more = "#zentools122 .zenmore";

			jQuery(more).hide();

			jQuery("#zentools122 li").hover(
				function() {
				 jQuery(this).find(".zenmore").fadeIn();
			},
			function(){
				 jQuery(this).find(".zenmore").fadeOut();
			});

		});
